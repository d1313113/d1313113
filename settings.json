      "vue-html": "html",
      "vue": "html",
      "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "workbench.colorTheme": "Monokai",
  "terminal.external.windowsExec": "C:\\#Alan\\Software\\cmder\\Cmder.exe",
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "html",
      "vue",
      "vue-html",
      {
          "language": "vue",
          "autoFix": true
      }
  ],
  // 需要 npm install -g eslint-plugin-html
  "eslint.options": {
      "plugins": [
          "html"
      ],
      "extensions": [
          ".js",
          ".vue"
      ]
  },
  "files.associations": {
      "*.vue": "vue",
      "*.js": "javascript",
      // "*.wpy": "vue"
  },
  "editor.cursorBlinking": "smooth",

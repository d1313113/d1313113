{
  "editor.fontSize": 16,
  "editor.fontFamily": "Verdana, Consolas", //Fira Code
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.formatOnType": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "extensions.autoUpdate": true,
  "emmet.syntaxProfiles": {
      "vue-html": "html",
      "vue": "html",
      // "javascript": "jsx",
      "javascript": "html"

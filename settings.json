  "vetur.format.styleInitialIndent": true,
  "vetur.format.scriptInitialIndent": true,
  "eslint.autoFixOnSave": true,
  "window.zoomLevel": 0,
  "sync.askGistName": false,
  "fileheader.Author": "Alan-Yih",
